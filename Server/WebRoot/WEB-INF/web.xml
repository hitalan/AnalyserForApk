<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
 
  <servlet>
    <servlet-name>ReceiveServlet</servlet-name>
    <servlet-class>org.hit.servlet.ReceiveServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AnalysisServlet</servlet-name>
    <servlet-class>org.hit.servlet.AnalysisServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AnalysisServlet</servlet-name>
    <url-pattern>/analysis</url-pattern>
  </servlet-mapping>	
  <servlet-mapping>
    <servlet-name>ReceiveServlet</servlet-name>
    <url-pattern>/receiveTask</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
